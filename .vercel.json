{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/panel",
      "dest": "/routes/panel"
    },
    {
      "src": "/(.*)",
      "dest": "/"
    },
    {
      "src": "/build/.*",
      "headers": { "cache-control": "public, max-age=31536000, immutable" },
      "continue": true
    },
    { "handle": "filesystem" },
    { "src": "/.*", "dest": "/index.js" }
  ],
  "framework": "remix",
  "buildCommand": "npm run build",
  "outputDirectory": "public"
}